<%- include('partials/header') %>

<main class="error-page">
  <div class="container">
    <h1>Error</h1>
    <p><%= message %></p>
    <% if (process.env.NODE_ENV !== 'production' && typeof error !== 'undefined') { %>
      <pre><%= error.stack %></pre>
    <% } %>
    <a href="/" class="btn btn-primary">Return Home</a>
  </div>
</main>

<%- include('partials/footer') %>